  "version": "2.0.0",
  "tasks": [
    {
      "label": "buildWebApp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/Frontend/VShop.Web/VShop.Web.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "buildProductApi",
      "command": "dotnet",
      "type": "process",
